<div class="center">
    <h1>List of stations</h1>

    <table border="1">
        <tr>
            <td>No station</td>
            <td>Station name</td>
        </tr>
        <c:forEach var="station" items="${stations}">
            <tr>
                <td><c:out value="${station.id}"/></td>
                <td><c:out value="${station.name}"/></td>
            </tr>
        </c:forEach>
    </table>
</div>